{
    "name": "jdefez/enum",
    "type": "package",
    "require-dev": {
        "laravel/pint": "^1.18",
        "larastan/larastan": "^3.0",
        "pestphp/pest": "^3.5"
    },
    "autoload": {
        "psr-4": {
            "Jdefez\\Enum\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Jdefez\\Enum\\Tests\\": "tests/"
        }
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
